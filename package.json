{
  "name": "shooting-victims",
  "version": "0.0.1",
  "description": "",
  "main": "js/src/app.js",
  "scripts": {
    "watch": "grunt watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "pip install -r dynamic/requirements.txt",
    "download": "run-p download:*",
    "download:homicides": "mkdir -p ./data/homicides && python dynamic/Genie_Script/genie_script.py data/homicides/homicides.csv -h",
    "download:shootings": "mkdir -p ./data/shootings && python dynamic/Genie_Script/genie_script.py data/shootings/shootings.csv -s",
    "process": "run-p process:*",
    "process:current-shootings": "node node_scripts/getCurrentYear.js ./data/shootings/shootings_geocode.csv ./data/shootings/shootings_geocode_current.csv",
    "process:current-homicides": "node node_scripts/getCurrentYear.js ./data/homicides/homicides_geocode.csv ./data/homicides/homicides_geocode_current.csv",
    "process:last365-shootings": "node node_scripts/getLast365.js ./data/shootings/shootings_geocode.csv ./data/shootings/shootings_geocode_last365.csv",
    "process:last365-homicides": "node node_scripts/getLast365.js ./data/homicides/homicides_geocode.csv ./data/homicides/homicides_geocode_last365.csv",
    "prebuild": "run-p prebuild:*",
    "prebuild:victims-homicides": "mkdir -p ./subtemplates/homicides && node node_scripts/makeVictimsList.js ./data/homicides/homicides_geocode_current.csv ./subtemplates/homicides/_victims-list.html",
    "prebuild:victims-shootings": "mkdir -p ./subtemplates/homicides && node node_scripts/makeVictimsList.js ./data/shootings/shootings_geocode_current.csv ./subtemplates/shootings/_victims-list.html 100",
    "prebuild:year-counter-homicides": "mkdir -p ./subtemplates/homicides && node node_scripts/makeYTDCounter.js ./subtemplates/homicides/_year-counter.html",
    "prebuild:year-counter-shootings": "mkdir -p ./subtemplates/shootings && node node_scripts/makeYTDCounter.js ./subtemplates/shootings/_year-counter.html",
    "prebuild:victim-filters-homicides": "mkdir -p ./subtemplates/homicides && node node_scripts/makeVictimFilters.js ./data/homicides/homicides_geocode_current.csv ./subtemplates/homicides/_victim-filters.html",
    "grunt": "mkdir -p css && grunt",
    "build:sequence": "run-s download process prebuild",
    "build": "run-p grunt build:sequence",
    "publish": "npm run build && tarbell publish production"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
