{
  "name": "shooting-victims",
  "version": "0.0.1",
  "description": "",
  "main": "js/src/app.js",
  "scripts": {
    "watch": "grunt watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "pip install -r dynamic/requirements.txt",
    "download": "npm run download:homicides && npm run download:shootings",
    "download:homicides": "mkdir -p ./data/homicides && python dynamic/Genie_Script/genie_script.py data/homicides/homicides.csv -h",
    "download:shootings": "mkdir -p ./data/shootings && python dynamic/Genie_Script/genie_script.py data/shootings/shootings.csv -s",
    "process": "npm run process:current-shootings && npm run process:current-homicides",
    "process:current-shootings": "node node_scripts/getCurrentYear.js ./data/shootings/shootings_geocode.csv ./data/shootings/shootings_geocode_current.csv",
    "process:current-homicides": "node node_scripts/getCurrentYear.js ./data/homicides/homicides_geocode.csv ./data/homicides/homicides_geocode_current.csv",
    "build": "npm run download && npm run process && mkdir -p css && grunt && npm run build:victims-homicides && npm run build:year-counter-homicides && npm run build:year-counter-shootings",
    "build:victims-homicides": "mkdir -p ./subtemplates/homicides && node node_scripts/makeVictimsList.js ./data/homicides/homicides_geocode.csv ./subtemplates/homicides/_victims-list.html",
    "build:year-counter-homicides": "mkdir -p ./subtemplates/homicides && node node_scripts/makeYTDCounter.js ./subtemplates/homicides/_year-counter.html",
    "build:year-counter-shootings": "mkdir -p ./subtemplates/shootings && node node_scripts/makeYTDCounter.js ./subtemplates/shootings/_year-counter.html",
    "build:victim-filters-homicides": "mkdir -p ./subtemplates/homicides && node node_scripts/makeVictimFilters.js ./data/homicides/homicides_geocode_current.csv ./subtemplates/homicides/_victim-filters.html",
    "publish": "npm run build && tarbell publish production"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Chicago Tribune News Applications",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^10.2.4",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-postcss": "^0.8.0",
    "grunt-sass": "^2.0.0",
    "html-minifier": "^3.5.2",
    "json2csv": "^3.11.1",
    "lodash.uniqby": "^4.7.0",
    "minifyify": "^7.3.3",
    "trib-styles": "git+https://tribune.unfuddle.com/git/tribune_trib-styles/"
  },
  "browserify": {
    "debug": true
  },
  "tribune": {},
  "dependencies": {
    "d3": "^4.9.1",
    "d3-queue": "^3.0.7",
    "leaflet": "^1.2.0",
    "leaflet-providers": "^1.1.17",
    "lodash.countby": "^4.6.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.sumby": "^4.6.0",
    "lodash.union": "^4.6.0",
    "nouislider": "^10.1.0",
    "waypoints": "^4.0.1"
  }
}
