    "grunt": "^0.4.5",
    "grunt-browserify": "^5.2.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-postcss": "^0.8.0",
    "grunt-sass": "^2.0.0",
    "html-minifier": "^3.5.2",
    "json2csv": "^3.11.1",
    "lodash.uniqby": "^4.7.0",
    "minifyify": "^7.3.3",
    "npm-run-all": "^4.1.1",
    "trib-styles": "git+https://tribune.unfuddle.com/git/tribune_trib-styles/"
  },
  "browserify": {
    "debug": true
  },
  "tribune": {},
